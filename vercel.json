{
    "version": 2,
    "builds": [
      {
        "src": "api_*.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      },
      {
        "src": "scripts.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.png",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api_index(/(.*))?",
        "dest": "/api_index.js"
      },
      {
        "src": "/api_upload(/(.*))?",
        "dest": "/api_upload.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }